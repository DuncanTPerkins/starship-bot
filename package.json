{
  "name": "starship-bot",
  "version": "1.0.0",
  "description": "A server bot for discord.",
  "main": "main.js",
  "scripts": {
    "build": "npx webpack-cli ./webpack.config.js && cd .config && cp package.json.empty ../build/package.json",
    "start": "ts-node-dev main.ts",
    "tsc": "npx tsc --project tsconfig.json && cp package.json package-lock.json build",
    "tsc-win": "npx tsc --project tsconfig.json && copy package*.json build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanTPerkins/starship-bot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "starship"
  ],
  "author": "Duncan Perkins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DuncanTPerkins/starship-bot/issues"
  },
  "homepage": "https://github.com/DuncanTPerkins/starship-bot#readme",
  "dependencies": {
    "@discordjs/rest": "0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.2.0",
    "ffmpeg-static": "^4.4.0",
    "libsodium-wrappers": "^0.7.9",
    "node": "^16.10.0",
    "node-loader": "^2.0.0",
    "path": "^0.12.7",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "ts-loader": "^9.2.6",
    "utf-8-validate": "^5.0.7",
    "ytdl-core": "^4.9.1",
    "ytsr": "^3.5.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.1",
    "@types/sqlite3": "^3.1.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
